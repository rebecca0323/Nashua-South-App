<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".X2Login"
    android:background="#B59DDC">

    <TextView
        android:id="@+id/class1"
        android:layout_width="154dp"
        android:layout_height="44dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="100dp"
        android:backgroundTint="@color/colorAccent"
        android:fontFamily="@font/amiko"
        android:text=""
        android:textColor="#FFF"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/class2"
        android:layout_width="154dp"
        android:layout_height="44dp"
        android:layout_marginStart="120dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="25dp"
        android:backgroundTint="@color/colorAccent"
        android:fontFamily="@font/amiko"
        android:textColor="#FFF"
        android:textSize="18dp"
        android:text=""
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/class1" />

    <TextView
        android:id="@+id/class3"
        android:layout_width="154dp"
        android:layout_height="44dp"
        android:layout_marginStart="120dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="25dp"
        android:backgroundTint="@color/colorAccent"
        android:fontFamily="@font/amiko"
        android:text=""
        android:textColor="#FFF"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/class2" />

    <TextView
        android:id="@+id/class4"
        android:layout_width="154dp"
        android:layout_height="44dp"
        android:layout_marginStart="120dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="25dp"
        android:text=""
        android:backgroundTint="@color/colorAccent"
        android:fontFamily="@font/amiko"
        android:textColor="#FFF"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/class3" />

    <TextView
        android:id="@+id/g1"
        android:layout_width="50dp"
        android:layout_height="21dp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="100dp"
        android:backgroundTint="#FFF"
        android:text=""
        android:fontFamily="@font/amiko"
        android:textColor="@color/colorAccent"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/class1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/g3"
        android:layout_width="50dp"
        android:layout_height="21dp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="50dp"
        android:text=""
        android:backgroundTint="#FFF"
        android:fontFamily="@font/amiko"
        android:textColor="@color/colorAccent"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/class3"
        app:layout_constraintTop_toBottomOf="@+id/g2" />

    <TextView
        android:id="@+id/g4"
        android:layout_width="50dp"
        android:layout_height="21dp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="50dp"
        android:backgroundTint="#FFF"
        android:text=""
        android:fontFamily="@font/amiko"
        android:textColor="@color/colorAccent"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/class4"
        app:layout_constraintTop_toBottomOf="@+id/g3" />

    <TextView
        android:id="@+id/g2"
        android:layout_width="50dp"
        android:layout_height="21dp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="50dp"
        android:backgroundTint="#FFF"
        android:text=""
        android:fontFamily="@font/amiko"
        android:textColor="@color/colorAccent"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@+id/class2"
        app:layout_constraintTop_toBottomOf="@+id/g1" />

    <Button
        android:id="@+id/showE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginLeft="162dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="161dp"
        android:layout_marginRight="161dp"
        android:background="@drawable/lunch_cal"
        android:backgroundTint="#412B7A"
        android:fontFamily="@font/alegreya_sans"
        android:onClick="goEbs"
        android:padding="10dp"
        android:text="Show Eblocks"
        android:textAlignment="center"
        android:textColor="#FFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/class4" />

    <TextView
        android:id="@+id/EblockList"
        android:layout_width="250dp"
        android:layout_height="200dp"
        android:layout_marginStart="115dp"
        android:layout_marginLeft="115dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="115dp"
        android:layout_marginRight="115dp"
        android:fontFamily="@font/amiko"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/showE" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="155dp"
        android:layout_height="42dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="31dp"
        android:background="#785AAD"
        android:fontFamily="@font/alegreya_sans1"
        android:text="Classes:"
        android:textAlignment="center"
        android:textColor="#FFF"
        android:textSize="26dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/show1"
        android:layout_width="57dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="27dp"
        android:background="@drawable/lunch_cal"
        android:text="B1"
        android:onClick="show1"
        android:textColor="@color/colorAccent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/show2"
        android:layout_width="57dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="25dp"
        android:onClick="show2"
        android:background="@drawable/lunch_cal"
        android:text="B2"
        android:textColor="@color/colorAccent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/show1" />

    <Button
        android:id="@+id/show3"
        android:layout_width="57dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:onClick="show3"
        android:layout_marginTop="20dp"
        android:background="@drawable/lunch_cal"
        android:text="B3"
        android:textColor="@color/colorAccent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/show2" />

    <Button
        android:id="@+id/show4"
        android:layout_width="57dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:onClick="show4"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/lunch_cal"
        android:text="B4"
        android:textColor="@color/colorAccent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/show3" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="28dp"
        android:clickable="true"
        android:onClick="backHome"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_revert" />

    <WebView
         android:id="@+id/x2webview"
         android:layout_width="match_parent"
         android:layout_height="match_parent"
         tools:layout_editor_absoluteX="-104dp"
         tools:layout_editor_absoluteY="0dp" >
    </WebView>


</androidx.constraintlayout.widget.ConstraintLayout>